{
  "name": "dev-package-template",
  "version": "0.0.1",
  "description": "A quick way to start developing react packages.",
  "main": "dist/index.js",
  "scripts": {
    "flow": "flow",
    "favicon": "cpx \"src/docs/favicon.*\" \"docs\"",
    "dev": "webpack-dev-server --config dev/webpack.config.js --mode development",
    "build:lib": "babel src/lib -d dist",
    "build:docs": "webpack --config dev/webpack.config.js --mode production",
    "build": "npm run build:lib && npm run build:docs && npm run favicon"
  },
  "keywords": [],
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.3.0 || ^16.2.0",
    "react-dom": "^15.3.0 || ^16.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "flow-bin": "^0.73.0",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3",
    "cpx": "^1.5.0"
  },
  "author": "Armando Sosa <arm.sosa@gmail.com>",
  "homepage": "https://github.com/soska/react-keyboardist",
  "repository": {
    "type": "git",
    "url": "git@github.com:soska/dev-package-template.git"
  }
}
